{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build && storybook build",
    "codegen": "chakra-cli tokens --strict-token-types --strict-component-types --template augmentation --out src/types/chakra.d.ts src/core/theme/index.ts",
    "dev": "vite --host",
    "doc": "storybook dev -p 6006",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "TZ=UTC vitest --watch=false",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@chakra-ui/styled-system": "2.9.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "framer-motion": "11.2.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.4.1",
    "@chromatic-com/storybook": "1.4.0",
    "@pidart/eslint-config": "workspace:*",
    "@storybook/addon-docs": "8.1.3",
    "@storybook/addon-essentials": "8.1.3",
    "@storybook/addon-interactions": "8.1.3",
    "@storybook/addon-links": "8.1.3",
    "@storybook/addon-onboarding": "8.1.3",
    "@storybook/blocks": "8.1.3",
    "@storybook/react": "8.1.3",
    "@storybook/react-vite": "8.1.3",
    "@storybook/test": "8.1.3",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint-plugin-storybook": "0.8.0",
    "storybook": "8.1.3",
    "typescript": "5.4.5",
    "vite": "5.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
